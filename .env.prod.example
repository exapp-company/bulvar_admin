APP_NAME=Laravel
APP_ENV=production
APP_KEY=base64:your-app-key-goes-here
APP_DEBUG=false
APP_TIMEZONE=UTC
APP_URL=https://your-production-url.com

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=daily
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=warning

DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_DATABASE=production_db_name
DB_USERNAME=prod_db_user
DB_PASSWORD=secure_password_here

SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=true
SESSION_PATH=/
SESSION_DOMAIN=.your-domain.com

BROADCAST_CONNECTION=redis
FILESYSTEM_DISK=s3
QUEUE_CONNECTION=redis

CACHE_STORE=redis
CACHE_PREFIX=your_app_cache_prefix

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=secure_redis_password_here
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=smtp.your-mail-provider.com
MAIL_PORT=587
MAIL_USERNAME=your-smtp-username
MAIL_PASSWORD=your-smtp-password
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS="no-reply@your-domain.com"
MAIL_FROM_NAME="${APP_NAME}"

WWWUSER=1000
WWWGROUP=1000

DOMOPLANER_API_URL=https://domoplaner.ru/api/
DOMOPLANER_API_TOKEN=your_secure_token_here
DOMOPLANER_FEED_URL=https://domoplaner.ru/dc-api/feeds/secure-feed-url/

JWT_SECRET=your_jwt_secret_here

COMPANY_NAME="Your Company Name"
TEAM_NAME="Your Team Name"
CONTACT_EMAIL="support@yourdomain.com"
CONTACT_PHONE="+1234567890"
